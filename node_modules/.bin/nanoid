#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ASUS/koc-tool/node_modules/nanoid/bin/node_modules:/mnt/c/Users/ASUS/koc-tool/node_modules/nanoid/node_modules:/mnt/c/Users/ASUS/koc-tool/node_modules:/mnt/c/Users/ASUS/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ASUS/koc-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ASUS/koc-tool/node_modules/nanoid/bin/node_modules:/mnt/c/Users/ASUS/koc-tool/node_modules/nanoid/node_modules:/mnt/c/Users/ASUS/koc-tool/node_modules:/mnt/c/Users/ASUS/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ASUS/koc-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
